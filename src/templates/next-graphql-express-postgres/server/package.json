{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "<%= projectDescription %>",
  "main": "index.js",
  "scripts": {
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "build": "tsc",
    "watch": "tsc -w",
    "nodemon": "nodemon dist/index.js",
    "nodemon2": "nodemon --exec ts-node src/index.ts",
    "dev": "npm-run-all --parallel  watch nodemon",
    "dev2": "npm-run-all --parallel watch nodemon2",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "client":"cd ../ && npm run dev --prefix client",
    "runall": "npm-run-all --parallel client  dev",
    "runall2": "npm-run-all --parallel client  dev2"
  },
  "keywords": [],
  "author": "<%= authorName %>",
  "license": "ISC",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "dependencies": {
    "apollo-server-express": "^2.19.0",
    "argon2": "^0.27.0",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "ioredis": "^4.19.2",
    "module-alias": "^2.2.2",
    "nodemailer": "^6.4.14",
    "path": "^0.12.7",
    "type-graphql": "^1.1.0",
    "typeorm": "^0.2.29",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/connect-redis": "0.0.14",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/ioredis": "^4.17.7",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^8.3.0",
    "gen-env-types": "^1.0.4",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "_moduleAliases": {
    "@": "dist/"
  }
}
